<div>
	 <table class="table table-striped table-condensed">
	<thead>
		<tr>
		    <th>Email</th>
			<th>Name</th>
			<th>Gender</th>
			<th>Country</th>
			<th>Age</th>
		</tr>
	</thead>
	<tbody id="user_list">
		<% User.all.each do |user| %>
		  <tr>
		      <td><%= link_to "#{user.id}. #{user.email}", messages_list_path(user_id: user.id) %></td>
			   <td><%= link_to "#{user.name}", messages_list_path(user_id: user.id) %></td>
			   <td><%= user.gender %></td>
			   <td><%= user.country %></td>
			   <td><%= Date.today.year - user.birth_date.to_date.year rescue nil %></td>
		  </tr>
		<% end %>
	</tbody>
	</table>
</div>